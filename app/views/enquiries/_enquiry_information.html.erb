          <!-- NEW Enquiry - STARTS -->
        <div class="boxp">
          <div >
        <!-- Enquiry INFORMATION - STARTS -->
          <div class="htab">
             <h5><b>Enquiry Information</b></h5>
        </div><br/>
          <div class="cols">
               <div class="col6">
                    <div class="fbox">
                         <%= f.label :first_name %>
                         <%= f.text_field :first_name,{tabindex: "1"} %>
                    </div>
                    <div class="fbox">
                          <%= f.label :mobile1 %>
                          <%= f.text_field :mobile1,{tabindex: "3"} %>
                    </div>
                    <div class="fbox">
                         <%= f.label :email1 %>
                         <%= f.text_field :email1,{tabindex: "5"} %>
                    </div>
                    <div class="fbox">
                         <%= f.label :gender %>
                         <% %w(Male Female).each do |elem| %>
                             <%= radio_button_tag "#{f.object_name}[gender]",
                                   elem,
                                   @enquiry.gender %>
                             <%= label_tag elem %>
                         <% end %>
                    </div>
                    <div class="fbox">
                         <%= f.label :country_of_origin %>
                         <%= f.collection_select(:country_id, 
                                Country.order(:name), 
                                :id, 
                                :name) %> &nbsp;&nbsp;
                    </div>
                    <div class="fbox">
                         <%= go_create(f,"student_source","source_id","name",1) %>  
                    </div>
                    
               </div>
               <div class="col6">
                    <div class="fbox">
                         <%= f.label :surname %>
                         <%= f.text_field :surname,{tabindex: "2"} %>
                    </div>
                    <div class="fbox">
                         <%= f.label :mobile2 %>
                         <%= f.text_field :mobile2,{tabindex: "4"} %>
                    </div>
                    <div class="fbox">
                         <%= f.label :email2 %>
                         <%= f.text_field :email2,{tabindex: "6"} %>
                    </div>
                    <div class="fbox">
                         <%= f.label :date_of_birth %>
                         <%= f.text_field :date_of_birth,{class: "dateField caltf ",tabindex: "7"} %>
                    </div>
                    <div class="fbox">
                         <%= f.label :score %>
                         <%= f.select :score,
                                   options_for_select((1..10)) %>

                    </div>
                    <%= fbox(f,[:assigned_to, 
                     User.order(:first_name), 
                     :id, 
                     :first_name,
                     {:prompt => "--User--"}],"collection_select",1,"Assign To") %>
  <%= f.hidden_field :assigned_by, :value => current_user.id.to_s %> 
               </div>
          </div><br/><br/>
        <!-- ENQUIRY INFORMATION - ENDS -->
        
      
          </div>
          <!-- NEW REGISTRATION - ENDS -->
    
     
