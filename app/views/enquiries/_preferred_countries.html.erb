<h4>Preferred countries</h4> 
<script>
$(document).ready(function(){
  $('#multiselect').multiSelect('deselect_all');
  <% @enquiry.countries.each do |i| %>
    $("#ms-multiselect .ms-selectable:first-child ul li[ms-value="+ <%= i.id %> + "]").trigger("click");
  <% end %>
  
});  
</script>
  Search<input type="text" size="25" class="searchKey"/>
  <div class="field">
      
    <a href='#' id='deselect-all'>deselect all</a>
    <%= select_tag("#{f.object_name}[country_ids][]",
                   options_for_select(@countries),
                   {multiple: true,
                    size: 5,
                    include_blank: false, 
                    id: "multiselect"}) %>
  </div>
