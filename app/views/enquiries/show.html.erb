<!-- Enquiry show page -->
<div class="col-md-8">
  <h3><%= @enquiry.name %></h3>
</div>

<% a = @enquiry %>
<div class="col-xs-4 text-right">
                <div class="form-group">
                  <%= link_to "Edit", edit_enquiry_path(a), {class: "btn btn-primary"} %>
                  <%= link_to "Clone", clone_enquiry_path(a), {class: "btn btn-primary"} %>
              </div>
</div>



<div class="row">
  <div class="col-xs-12">
      <ul class="nav nav-pills seperator">
        <li id="basic" class="active">
          <%= link_to "Basic","/enquiries/#{a.id}" %>
        </li>
        <%= render "actions" %>
      </ul>
      <!-- some html5 data to be sent to the back end -->
      <div id="enquiry_id_div" data-enquiry_id='<%= a.id %>' data-model_sing='enquiry' data-model_plu='enquiries' > </div>

      <div id="action_div">
        <%= render @partial, @locals %>
      </div>
  </div>
</div>



<script>

 actmm('enquiries');

</script>
