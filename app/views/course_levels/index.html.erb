<h1>Listing course_levels</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Desc</th>
    <th>Created by</th>
    <th>Updated by</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @course_levels.each do |course_level| %>
  <tr>
    <td><%= course_level.name %></td>
    <td><%= course_level.desc %></td>
    <td><%= course_level.created_by %></td>
    <td><%= course_level.updated_by %></td>
    <td><%= link_to 'Show', course_level %></td>
    <td><%= link_to 'Edit', edit_course_level_path(course_level) %></td>
    <td><%= link_to 'Destroy', course_level, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Course level', new_course_level_path %>
