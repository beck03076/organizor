<script type="text/javascript">
$(function(){
  $("ul.tab-menus li.launch").show();
  $(".tab-menus li").removeClass("active-tab");
  $("ul.tab-menus li.launch").addClass("active-tab");
  $("ul.tab-menus li.launch").text('<%= @registration.first_name.titleize %>');
  $("ul.tab-menus li.launch").attr('data-registration_id', '<%= @registration.id %>');
  $("ul.tab-menus li.launch").trigger("click");
});

</script>

