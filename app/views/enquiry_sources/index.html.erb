<h1>Listing enquiry_sources</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Desc</th>
    <th>Created by</th>
    <th>Updated by</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @enquiry_sources.each do |enquiry_source| %>
  <tr>
    <td><%= enquiry_source.name %></td>
    <td><%= enquiry_source.desc %></td>
    <td><%= enquiry_source.created_by %></td>
    <td><%= enquiry_source.updated_by %></td>
    <td><%= link_to 'Show', enquiry_source %></td>
    <td><%= link_to 'Edit', edit_enquiry_source_path(enquiry_source) %></td>
    <td><%= link_to 'Destroy', enquiry_source, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

