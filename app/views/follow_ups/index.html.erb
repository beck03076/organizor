                              {class: "btn btn-primary",
                               data: { confirm: 'This can take a few moments, are you sure?' }} %>
                </div>
              </div>
              <% end %>
</div>


            <div class="row">
              <div class="col-md-12">
                <div class="well">
                  <div id='calendar'> 
                  </div>
                        <div class='cal_click_container'> 
                        </div>
                </div>
              </div>
            </div>

