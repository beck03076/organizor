<%= simple_form_for(@follow_up) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :title %>
    <%= f.input :desc %>
    <%= f.input :starts_at %>
    <%= f.input :ends_at %>
    <%= f.input :api %>
    <%= f.input :event_type_id %>
    <%= f.input :venue %>
    <%= f.input :remind_before %>
    <%= f.input :created_by %>
    <%= f.input :updated_by %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
