<!-- only contracts group -->
<% f.object.contracts.build if f.object.contracts.blank? %>
<%= f.fields_for :contracts do |c| %>
    <% arr.each_with_index do |element,i| %>
      <div id="id_part_<%= i + index + 1 %>" class="<%= set %>">
        <h4 class="form-heading" id="<%= element.to_s %>"><%= element.to_s.titleize %></h4>
          <div class="row"></div>

            <%= render 'form_' + element.to_s, c: c %>

      </div>
    <% end %>
<% end %>

